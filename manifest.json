{
    "manifest_version":2,
    "name":"Chrome ExKittension",
    "version":"1.0.0.0",
    "description":"A kitten extension",
    "web_accessible_resources":["kit/*.jpg"],
    "content_scripts":[{
        "matches":["<all_urls>"],
        "js":["kitten.js"]
    }],
    "icons":{"128":"icon128.png","48":"icon48.png","16":"icon16.png"},
    "browser_action":{"default_icon":"icon16.png","default_popup":"popup.html"}
}